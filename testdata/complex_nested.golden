package main

import "fmt"

type Person struct {
	Name string
	Age  int
}

func processData(people []Person, filter func(Person) bool) []string {
	var names []string
	for _, p := range people {
		if filter(p) {
			names = append(names, p.Name)
		}
	}
	return names
}

func main() {
	people := []Person{{Name: "Alice", Age: 30}, {Name: "Bob", Age: 25}}

	adults := processData(people, func(p Person) bool {
		return p.Age >= 18
	})

	fmt.Println(adults)
}
